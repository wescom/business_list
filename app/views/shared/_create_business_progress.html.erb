<div class="col-md-12 progress_block">
	<div class="d-flex justify-content-center text-center">
	  <div class="col">Name</div>
	  <div class="col">Location</div>
	  <div class="col">Services</div>
	  <div class="col">Extras</div>
	  <div class="col">Contacts</div>
	</div>
	<div class="progress" style="height: 20px;" >
    <div class="dot-one info-color"></div>
		<div class="dot-two no-color">
		</div>
		<div class="dot-three no-color"></div>
		<div class="dot-four no-color"></div>
		<div class="dot-five no-color"></div>
    <div class="progress-bar info-color" role="progressbar" aria-valuenow="<%= @progress %>" aria-valuemax="100" style="width: <%= @progress %>%" ></div>
  </div>
</div>